<view class="my_title">
  <!-- logo-->
  <navigator class="logo">
    <image class="logo_img" style="width:160rpx;height:85rpx" src="../../icons/logo.jpg"></image>
  </navigator>
  <!-- 搜索-->
  <view class="search_box">
    <input placeholder="请输入搜索" bindinput="getInput" value="{{val}}"></input>
    <icon type="search" wx:if="{{isSearch}}"></icon>
    <icon type="clear" wx:if="{{isClear}}" bindtap="clearTap"></icon>
   </view>
  <!-- 头像-->
  <view class="user_icon">
    <image style="width:68rpx;height:68rpx" src="../../icons/touxiang.jpg"></image>
   </view>
  <!-- 下载-->
  <view class="down_app">
    <button type="defaule" bindtap="clickButton">下载</button>
  </view>
</view>